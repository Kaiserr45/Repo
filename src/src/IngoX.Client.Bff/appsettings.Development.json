{
	// Настройки логирования
	"IngosLogging": {
		"LogConsoleTarget": {
			"MinLogLevel": "Trace",
			"Mode": "Standard"
		},
		// Логирование в порт http
		"LogHttpTarget": {
			"Enabled": false,
			"RequestUri": "http://msvo-elktst.corp.ingos.ru:5049",
			"MinLogLevel": "Debug"
		},
		"LogFileTarget": {
			"Path": "/var/log/services",
			"ArchiveEvery": "Day",
			"ArchiveAboveSize": null,
			"MaxArchiveFiles": 30,
			"MinLogLevel": "Trace",
			"FlushToDiskInterval": 10,
			"Enabled": true
		}
	},
	"IndividualClientApiConfiguration": {
		"BaseUrl": "http://localhost:9001"
	},

	// Настройки вывода данных в модели ошибки
	"IngosErrorsHandling": {
		"ExceptionStackTraceIncluded": true, // Флаг вывода информации о трассировке,
		"ExceptionMessageIncluded": true // Флаг вывода сообщения об ошибке
	},

	"IngosApiSpecification": {
		"Enabled": true
	},

	"IngosRedisOptions": {
		// Тестовый для сервисов в DC "Войковская"
		"EndPoints": "msvo-redis01tst.corp.ingos.ru:6379, msln-redis01tst.corp.ingos.ru:6379"
	},

	"IngosCaptcha": {
		"Enabled": false,
		"BaseUrl": "http://test-int-gate.corp.ingos.ru"
	},

	"AisGate": {
		"EndPoints": "https://aisgate-test.msk.ingos.ru/igsSvcGate.svc, https://aisgate-test.msk.ingos.ru:1443/igsSvcGate.svc, https://aisgate-test.msk.ingos.ru:2443/igsSvcGate.svc"
	},

	"IngosAuthorization": {
		"Validation": {
			"Issuer": "https://ssotest.ingos.ru"
		}
	},

	"Vault": {
		"Path": "ingos.services/test" // Путь в Vault до приложения и его среда (test / stage / prod)
	},

	"IndividualClientApi": {
		"BaseUrl": "https://test-int-gate.corp.ingos.ru/api/ingox-mdm/v1"
	}
}
